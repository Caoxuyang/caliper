{
    "blockchain": {
        "type": "parity",
        "config": "benchmark/parity_test/westlake.json"
      },
      
    "test": {
        "name": "westlake remote rpc test",
        "description" : "Benchmark for westlake chain scenario",
        "clients": {
          "type": "local",
          "number": 1
        },
    "rounds": [{
          "label" : "sendRawTx",
          "txNumber" : [1],
          "arguments": {"txsPerClient" : 1, "maxSocketsPerClient":100},
          "rateControl" : [{"type": "fixed-rate", "opts": {"tps" : 0}}],
          "callback" : "benchmark/parity_test/testSendRawTx.js"
        }]
      },
      "monitor": {
        "type": ["process"],
        "process": [
          {
            "command" : "node",
            "arguments" : "local-client.js",
            "multiOutput" : "avg"
          }
        ],
        "interval": 1
      }
}